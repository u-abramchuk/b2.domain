{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "dependencies": {
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta2-build3300",
        "b2.Domain": "1.0.0",
        "Microsoft.NETCore.Runtime.CoreCLR":"1.0.4"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
            "include": [
                "xunit.runner.json"
            ]
        },
        "debugType": "portable"
    },
    "tooling": {
        "defaultNamespace": "b2.Domain.Tests"
    },
    "tools": {
        "Microsoft.DotNet.Watcher.Tools": "1.0.0-*"
    }
}